{
  "name": "recommender",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "dev": "concurrently \"next dev --turbopack\" \"npm run start-flask\"",
    "build": "next build --turbopack",
    "start": "concurrently \"npm run start-next\" \"npm run start-flask\"",
    "start-next": "next start",
    "start-flask": "python ./recommender.py",
    "lint": "eslint"
  },
  "dependencies": {
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
